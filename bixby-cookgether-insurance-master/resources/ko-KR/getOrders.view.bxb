result-view{
  match:OrderResult(this)

  message{
    template ("주문 내역")
  }

  render {
    layout {
      section {
        content {
          for-each (this){
            as(order){
              divider
              layout-macro (OrderListSummary){
                param(order){
                  expression (order)
                }
              }
              divider     
            }
          }
        }
      }
    }
  }

  conversation-drivers{
    conversation-driver{
      template ("주문 내역")
    }
  }
}