layout-macro-def(StockListSummary) {
  params {
    param (stock) {
      type (StockResult)
      min (Required)
    }
  }
 
  content {
    compound-card{
      content{
        paragraph {
          value ("품목 : #{value(stock.stockname)}")
          style (Title_L)
        }
        paragraph {
          value ("재고량 :  #{value(stock.count)}")
          style (Title_L)
        }
      }
    }
  }
}
